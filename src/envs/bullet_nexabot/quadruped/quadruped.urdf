<?xml version="1.0"?>
<robot name="quadruped">

  <link name="torso">
    <visual>
      <geometry>
        <box size="0.2 0.1 0.05"/>
      </geometry>
      <origin xyz="0 0 0"/>
      <material name="green">
        <color rgba="0 0.8 .8 1"/>
      </material>
    </visual>
    <inertial>
      <mass value="0"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

    <link name="femur_lr">
      <visual>
        <geometry>
          <cylinder radius="0.01" length="0.1"/>
        </geometry>
        <origin xyz="0 0 0"/>
        <material name="green">
          <color rgba="0 0.8 .8 1"/>
        </material>
      </visual>
      <inertial>
        <mass value="0"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
      </inertial>
    </link>

  <joint name="torso_femur_lr" type="prismatic">
    <axis xyz="1 0 0"/>
    <origin xyz="0.0 0.0 0.0"/>
    <parent link="torso"/>
    <child link="femur_lr"/>
    <limit effort="100.0" lower="-3" upper="3" velocity="5"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

</robot>